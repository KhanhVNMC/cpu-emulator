{
    "scopeName": "source.fl516",
    "patterns": [
        {
            "name": "keyword.control.fl516",
            "match": "(?i)\\b(MOV|LDI|LDM|STO|STOI|ADD|ADDI|SUB|SUBI|MUL|MULI|DIV|DIVI|MOD|MODI|OR|ORI|XOR|XORI|SHR|SHRI|SHL|SHLI|NOT|PUSH|IPUSH|POP|HLT|JMP|CALL|RET|CMP|JEQ|JLT|JGT|JGE|JLE|NOP|DBGP|BLNK)\\b"
        },
        {
            "name": "variable.register.fl516",
            "match": "(?i)\\b(R[0-8]|RSP)\\b"
        },
        {
            "name": "constant.numeric.fl516",
            "match": "\\b(0x[0-9A-Fa-f]+|0b[01]+|\\d+)\\b"
        },
        {
            "name": "entity.name.function.fl516",
            "match": "\\.(\\w+):?"
        },
        {
            "name": "comment.line.semicolon.fl516",
            "match": ";.*$",
            "captures": {
                "0": { "name": "comment.line.semicolon.fl516" }
            }
        }
    ]
}
